<h1>Diretores</h1>
<%= link_to "new director", new_director_path%>
<ul>
    <% @directors.each do |director|%>
        <li>
            <%= link_to director.name, director_path(director)%>
            <%= link_to "editar", edit_director_path(director)%>
            <%= link_to "delete", director_path(director), data: {turbo_method: :delete}%>
        </li>
    <% end %>
</ul>

